<div class="row">
  <div class="input-field col s12">
    <%= form.label :email %>
    <%= form.email_field :email, autofocus: true %>
  </div>
</div>

<div class="row">
  <div class="input-field col s12">
    <%= form.label :password, "Senha", class: "validate"%>
    <%= form.password_field :password, autocomplete: "off" %>
  </div>
</div>

<% if devise_mapping.rememberable? -%>
  <div class="row">
    <div class="field col s12">
      <%= form.check_box :remember_me, class: "filled-in" %>
      <%= form.label :remember_me, "Lembrar de mim" %>
    </div>
  </div>
<% end -%>

<div class="waves-effect waves-light btn">
  <%= form.submit "Entrar" %>
</div>

<div class="login__links">
  <%= render "users/shared/links" %>
</div>
